#!/bin/bash

if [ ! -d "KNN" ]; then
  # Control will enter here if $DIRECTORY doesn't exist.
  mkdir KNN
  mkdir KNN/10
  mkdir KNN/67
fi

t=67


#weights =["uniform","distance"]

for (( c=1; c<=15; c++ ))
do  
   python Activity1-KNN.py 1 $t 0 $c > KNN/$t/NroTraining_$t-Machine_KNN-Distance_uniform-Vecinos_$c.txt
done

for (( c=1; c<=15; c++ ))
do  
   python Activity1-KNN.py 1 $t 1 $c > KNN/$t/NroTraining_$t-Machine_KNN-Distance_distance-Vecinos_$c.txt
done





## python Activity1-KNN.py 1 0 1 1 > KNN/NroTraining_10-Machine_KNN-B$c.txt

